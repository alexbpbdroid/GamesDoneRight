<!DOCTYPE html>
<html>
  <head>
    <title>GDR</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <link href="https://fonts.googleapis.com/css2?family=Lato&display=swap" rel="stylesheet">
    
    <script type="text/javascript">window.logoURL="<%= image_url("gog-logo.png")%>"</script>
    <script type="text/javascript">window.logo2URL="<%= image_url("goglogo2.png")%>"</script>
    <script type="text/javascript">window.arrowURL="<%= image_url("arrowdown.png")%>"</script>
    <script type="text/javascript">window.cartURL="<%= image_url("cart-icon.png")%>"</script>
    <script type="text/javascript">window.searchURL="<%= image_url("search-icon.png")%>"</script>
    <script type="text/javascript">window.dropGamesURL="<%= image_url("dropGames.png")%>"</script>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
